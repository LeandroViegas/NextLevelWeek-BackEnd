{
    "version": 2,
    "builds": [
        {"src": "/(.*)", "use": "@now/node"},
        {"src": "/src/database(.*)", "use": "@now/static"},
        {"src": "/uploads/(.*)", "use": "@now/static"}
      ],
    "routes": [
        {
            "src": "/**", 
            "dest": "index.ts"
        }
    ]
  }